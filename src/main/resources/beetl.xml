<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

	<bean class="tk.qw4wer.codeGenerate.utils.beetls.BeetlGroupUtilConfiguration" init-method="init">
		<property name="templateDir" value="/template"></property>
		<property name="prefix" value="fn"></property>
		<property name="fns">
			<map>
				<entry key="upperCaseFristChar" value="tk.qw4wer.codeGenerate.utils.beetls.functions.UpperCaseFristChar"/>
				<entry key="firstLowerUnderUpper" value="tk.qw4wer.codeGenerate.utils.beetls.functions.FirstLowerUnderUpper"/>
				<entry key="lowerCaseFirstChar" value="tk.qw4wer.codeGenerate.utils.beetls.functions.LowerCaseFirstChar"/>
			</map>
		</property>
	</bean>



</beans>